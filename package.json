    "url": "https://github.com/boyanio/angular-wasm.git"
  },
  "keywords": [
    "angular",
    "WebAssembly"
  ],
  "author": "Boyan Mihaylov",
  "devDependencies": {
    "@angular/common": "^5.2.1",
    "@angular/compiler": "^5.2.1",
    "@angular/compiler-cli": "^5.2.1",
    "@types/node": "~6.0.60",
    "@types/webassembly-js-api": "0.0.1",
    "codelyzer": "^4.0.1",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  },
  "dependencies": {
    "@angular/core": "^5.2.1",
    "rxjs": "^5.5.2"
  }
}